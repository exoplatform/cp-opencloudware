<h4 class="opencloudwareTitle">Evaluation :</h4>

<% if (flash.getError() != "") { %>
    <div id="flashMessage" class="alert-error pull-left flash">
        ${flash.getError()}
    </div>
<% } %>
<% if (flash.getSuccess() != "") { %>
    <div id="flashMessage" class="alert-success pull-left flash">
        <strong>${flash.getSuccess()}</strong>
    </div>
<% } %>