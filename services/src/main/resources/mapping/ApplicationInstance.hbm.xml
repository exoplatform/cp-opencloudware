<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

  <class name="org.opencloudware.hibernate.model.ApplicationInstance"
         table="ocw_applicationInstance">
    <cache usage="transactional"/>
    <id name="id"
        type="java.lang.Long"
        access="field">
      <column name="ID"/>
      <generator class="native"/>
    </id>

	  <many-to-one name="application" class="org.opencloudware.hibernate.model.Application" fetch="select">
		  <column name="APPLICATION_ID" not-null="true" />
	  </many-to-one>
  </class>
</hibernate-mapping>
