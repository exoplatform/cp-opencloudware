<page-set>
	<page>
		<name>opencloudwareMyOrganization</name>
		<title>My Organizations</title>
		<access-permissions>*:@owner@</access-permissions>
		<edit-permission>*:/platform/administrators</edit-permission>
			<portlet-application>
				<portlet>
					<application-ref>opencloudware-portlet</application-ref>
					<portlet-ref>MyOrganizationPortlet</portlet-ref>
				</portlet>
				<title>My Organization</title>
				<access-permissions>Everyone</access-permissions>
				<show-info-bar>false</show-info-bar>
				<show-application-state>false</show-application-state>
				<show-application-mode>false</show-application-mode>
			</portlet-application>
	</page>
