<node-navigation
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.gatein.org/xml/ns/gatein_objects_1_2 http://www.gatein.org/xml/ns/gatein_objects_1_2"
    xmlns="http://www.gatein.org/xml/ns/gatein_objects_1_2">
  <priority>1</priority>
  <page-nodes>
    <node>
      <uri>homepage</uri>
      <name>homepage</name>
      <label>Homepage</label>
      <visibility>DISPLAYED</visibility>
      <page-reference>portal::opencloudware::homepage</page-reference>
    </node>
	  <node>
		  <uri>services</uri>
		  <name>Services</name>
		  <label>Services Access</label>
		  <visibility>DISPLAYED</visibility>
		  <page-reference>portal::opencloudware::serviceAccess</page-reference>
	  </node>
	  <node>
		  <uri>join</uri>
		  <name>join</name>
		  <label>Join Organization</label>
		  <visibility>DISPLAYED</visibility>
		  <page-reference>portal::opencloudware::join</page-reference>
	  </node>
	  <node>
      <uri>detail</uri>
      <name>detail</name>
      <label>Detail</label>
      <visibility>SYSTEM</visibility>
      <page-reference>portal::opencloudware::detail</page-reference>
    </node>

    <node>
      <uri>printviewer</uri>
      <name>printviewer</name>
		<label>Print</label>
      <visibility>SYSTEM</visibility>
      <page-reference>portal::opencloudware::printviewer</page-reference>
    </node>

    <node>
      <uri>searchResult</uri>
      <name>searchResult</name>
		<label>Search Result</label>
      <visibility>HIDDEN</visibility>
      <page-reference>portal::opencloudware::searchResult</page-reference>
    </node>
  <!-- Unified Search page -->
  <node>
    <uri>search</uri>
    <name>search</name>
	  <label>Unified Search</label>
	  <visibility>HIDDEN</visibility>
    <page-reference>portal::opencloudware::search</page-reference>
  </node>
  <!-- File explorer -->
  <node>
    <uri>documents</uri>
    <name>documents</name>
	  <label>Documents</label>
	  <visibility>HIDDEN</visibility>
    <page-reference>portal::opencloudware::documents</page-reference>
  </node>
	  <node>
		  <uri>register</uri>
		  <name>register</name>
		  <label>Register</label>
		  <visibility>HIDDEN</visibility>
		  <page-reference>portal::opencloudware::register</page-reference>
	  </node>

  </page-nodes>
</node-navigation>
